		};

		/* TE SIGNAL */
		decon_te_on: decon_te_on {
			samsung,pins = "gpe0-2";
			samsung,pin-function = <2>;
		};

		decon_te_off: decon_te_off {
			samsung,pins = "gpe0-2";
			samsung,pin-function = <0>;
		};

		/* I2C_MUIC */
		i2c1_bus: i2c1-bus {
			samsung,pins = "gpc1-3", "gpc1-2";
			samsung,pin-function = <2>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};

		/* I2C_FUELGAUGE */
		i2c4_bus: i2c4-bus {
			samsung,pins = "gpc4-1", "gpc4-0";
			samsung,pin-function = <2>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
