		};

		gpf0_gpio: gpf0 {
			gpio-controller;
			#gpio-cells = <2>;

			interrupt-controller;
			#interrupt-cells = <2>;
		};

		gpf1_gpio: gpf1 {
			gpio-controller;
			#gpio-cells = <2>;

			interrupt-controller;
